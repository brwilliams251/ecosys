use graph MinHash
drop job load_hash
CREATE LOADING JOB load_hash FOR GRAPH MinHash {
      DEFINE FILENAME MyDataSource;
      LOAD MyDataSource TO TEMP_TABLE t1(vertexID, signature, stringvalue) VALUES($0, FLATTEN(minHash($1,"3","5"), "|", 1), $1) USING SEPARATOR="|", HEADER="true", EOL="\n";
      LOAD TEMP_TABLE t1 TO EDGE Has_Signature VALUES($"vertexID", $"signature");
}
